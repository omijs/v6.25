{
  "name": "@omiu/toast",
  "version": "0.0.13",
  "description": "Pop up prompt box.",
  "docsExtend": {
    "cnName": "弹出式提示",
    "cnDescription": "居中显示的弹出式提示",
    "codepen": "YzyVwOO",
    "codepenHeight": 391,
    "codepenDefaultTab": "html,result"
  },
  "main": "dist/index.es.js",
  "exports": {
    ".": "./dist/index.es.js"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "start": "yarn watch & vite",
    "dev": "yarn start",
    "build": "vite build && yarn build:scss",
    "build:demo": "vite build",
    "build:scss": "sass src/index.scss src/index.css",
    "watch": "sass --watch src/index.scss src/index.css --no-source-map",
    "pretest": "yarn build",
    "test": "web-test-runner \"test/*.js\" --node-resolve"
  },
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {
    "@omiu/common": "latest",
    "@omiu/loading": "^0.0.3",
    "@omiu/transition": "latest",
    "omi": "latest"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.5.33",
    "@types/node": "^16.4.13",
    "@web/test-runner": "^0.13.15",
    "sass": "^1.37.5",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  },
  "license": "MIT",
  "keywords": [
    "omi",
    "omiu"
  ]
}
